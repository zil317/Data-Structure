    while(high>=low){
      int middle = (low +high)/2;
      if (numberList[middle] == numberWord){
        return middle;
      }
      if(number[middle] < numberWord){
        low = middle+1;
      }
      if(number[middle] > numberWord){
        high = middle-1;
      }
      return -1;
    }
  }
  
